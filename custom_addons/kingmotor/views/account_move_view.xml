<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_move_form_inherit_kingmotor" model="ir.ui.view">
        <field name="name">account.move.form.inherit.kingmotor</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/header" position="inside">
                <button name="action_print_kingmotor_invoice"
                        string="Print Invoice"
                        type="object"
                        class="btn-primary"
                        icon="fa-print"/>
            </xpath>
        </field>
    </record>
</odoo>
