<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="km_transaction_line_view_form" model="ir.ui.view">
    <field name="name">km.transaction.line.form</field>
    <field name="model">km.transaction.line</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <group>
              <field name="product_id"/>
            </group>
            <group>
              <field name="quantity"/>
            </group>
            <group>
              <field name="price"/>
            </group>
            <group>
              <field name="subtotal"/>
            </group>
          </group>
        </sheet>
      </form>
    </field>
  </record>

</odoo>
